<script>
var ONEILL = window.ONEILL || {};

ONEILL.populateVeroCartProducts = function() {
    var $ = jQuery;
    var products = [];

    $('#mini-shopping-cart-table tr').each(function() {

        $container = $(this);

        products.push({
            Url: $container.data('vero-url'), // Use SAME COLOR as product being purchased for the URL.
            Image_url: $container.data('vero-image-url'), // Use SAME COLOR as product being purchased for the image URL. Large or Medium size image, as this can be inserted in dynamic emails.
            Color: $container.data('vero-color'),
            "Label_Desc__(Order_Detail)": $container.data('vero-label-name'),
            Size: $container.data('vero-size'),
            Division_Description: $container.data('vero-division-name'),
            Gender_Descripton: $container.data('vero-division-gender'),
            // "Gender_(Style's_Division)": $container.data('vero-division-gender'),
            Style_Description: $container.data('vero-style-description'),
            Style: $container.data('vero-style'),
            Season: $container.data('vero-season')
        });
    });

    return products;

};

ONEILL.veroCheckoutReidentify = function() {
    if(jQuery('#billing\\:email').val()) {
        _veroq.push(['reidentify', jQuery('#billing\\:email').val()]);
    }

    if(jQuery('#billing\\:email').val()) {
        //identify/update the user!!
        var _veroData = {
            id: jQuery('#billing\\:email').val(),
            email: jQuery('#billing\\:email').val(),
            name: jQuery('#billing\\:firstname').val() + ' ' + jQuery('#billing\\:lastname').val()
        };
        if (typeof VERO !== 'undefined') {
            VERO.storeEmail(jQuery('#billing\\:email').val());
            if (VERO.abandonedCartUrl) {
                _veroData.active_cart = VERO.abandonedCartUrl;
            }
        }
        _veroq.push(['user', _veroData]);
    }
};


jQuery(function($){

    $(document).on('checkout:step', function(evnt, step) {

        switch(step) {
            case 'begin':
            case 'billing':
                var products = ONEILL.populateVeroCartProducts();
                if(products.length) {
                    _veroq.push(['track', 'Begins checkout', {
                        products: products
                    }]);
                }
                break;
            case 'shipping_method':
                //re-identify the current user for safety
                ONEILL.veroCheckoutReidentify();
                _veroq.push(['track', step]);
                break;
            default:
                _veroq.push(['track', step]);
                break;
        }

    });

    /**
     * Product Added to Cart. Check catalog/product/view.phtml and quickview.phtml for trigger location
     * @date   2015-06-26T14:42:40-0600
     * @author Mc
     */
    $(document).on('cart:shown', function() {

        //loop through the product currently in the mini-cart
        var products = ONEILL.populateVeroCartProducts();

        if(products.length) {
            _veroq.push(['track', 'Viewed cart', {
                products: products
            }]);

            if (typeof VERO !== 'undefined') {
                VERO.loadScripts();
            }
        }

    });

    /**
     * Carousel images have been filtered after choosing color swatch. Check /js/script.js for trigger location
     * @return {[type]}                 [description]
     * @date   2015-06-26T14:43:37-0600
     * @author Mc
     */
     $(document).on('carousel:filtered', '.swatch', function() {
        $swatch = jQuery(this); //clicked anchor
        $container = jQuery('.product-init-container'); //product container

        _veroq.push(['track', 'Viewed product', {
            Url: $swatch.attr('href'), // Use SAME COLOR as product being purchased for the URL.
            Image_url: jQuery('.product-img-container .item:first img:first').attr('src'), // Use SAME COLOR as product being purchased for the image URL. Large or Medium size image, as this can be inserted in dynamic emails.
            Color: $swatch.data('color'),
            "Label_Desc__(Order_Detail)": $container.data('vero-label-name'),
            Division_Description: $container.data('vero-division-name'),
            Gender_Descripton: $container.data('vero-division-gender'),
            // "Gender_(Style's_Division)": $container.data('vero-division-gender'),
            Style_Description: jQuery('.product-details h1').text(),
            Style: $container.data('vero-style'),
            Season: $swatch.data('season')
        }]);
    });

});
</script>
