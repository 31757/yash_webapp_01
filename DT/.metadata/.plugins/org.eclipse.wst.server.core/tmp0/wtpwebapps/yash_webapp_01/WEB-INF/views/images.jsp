<div class="container-fluid">
  <br>
  <div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="2" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="3" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="4" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="5" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="6" data-interval="150"></li>
      <li data-target="#myCarousel" data-slide-to="7" data-interval="150"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <img src="./resources/images/Lighthouse.jpg" width=1400px height=250px>
      </div>

      <div class="item">
        <img src="./resources/images/Penguins.jpg" width=1400px height=250px>
      </div>
    
      <div class="item">
        <img src="./resources/images/Chrysanthemum.jpg" width=1400px height=250px>
      </div>

      <div class="item">
        <img src="./resources/images/Tulips.jpg" width=1400px height=250px>
      </div>
      
       <div class="item">
        <img src="./resources/images/Desert.jpg" width=1400px height=250px>
      </div>
      
       <div class="item">
        <img src="./resources/images/Hydrangeas.jpg" width=1400px height=250px>
      </div>
      
       <div class="item">
        <img src="./resources/images/Jellyfish.jpg" width=1400px height=250px>
      </div>
      
       <div class="item">
        <img src="./resources/images/Koala.jpg" width=1400px height=250px>
      </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>



















